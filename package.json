  "peerDependencies": {
    "next": "^14.2.5",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "overrides": {
    "@testing-library/react-hooks": {
      "@types/react": ">17",
      "react": ">17",
      "react-dom": ">17"
