    "lodash-es": "4.17.21",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "rollup": "4.1.4",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-preserve-directives": "^0.4.0",
